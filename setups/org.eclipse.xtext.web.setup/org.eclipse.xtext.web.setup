<?xml version="1.0" encoding="UTF-8"?>
<setup:Project
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:git="http://www.eclipse.org/oomph/setup/git/1.0"
    xmlns:predicates="http://www.eclipse.org/oomph/predicates/1.0"
    xmlns:setup="http://www.eclipse.org/oomph/setup/1.0"
    xmlns:setup.p2="http://www.eclipse.org/oomph/setup/p2/1.0"
    xmlns:setup.targlets="http://www.eclipse.org/oomph/setup/targlets/1.0"
    xmlns:setup.workingsets="http://www.eclipse.org/oomph/setup/workingsets/1.0"
    xsi:schemaLocation="http://www.eclipse.org/oomph/setup/git/1.0 http://git.eclipse.org/c/oomph/org.eclipse.oomph.git/plain/setups/models/Git.ecore http://www.eclipse.org/oomph/predicates/1.0 http://git.eclipse.org/c/oomph/org.eclipse.oomph.git/plain/setups/models/Predicates.ecore http://www.eclipse.org/oomph/setup/targlets/1.0 http://git.eclipse.org/c/oomph/org.eclipse.oomph.git/plain/setups/models/SetupTarglets.ecore http://www.eclipse.org/oomph/setup/workingsets/1.0 http://git.eclipse.org/c/oomph/org.eclipse.oomph.git/plain/setups/models/SetupWorkingSets.ecore"
    name="xtext.web"
    label="XtextWeb">
  <setupTask
      xsi:type="setup:VariableTask"
      name="xtext.nightly.composite"
      value="http://services.typefox.io/open-source/jenkins/job/xtext-eclipse/job/master/lastStableBuild/artifact/build/p2-repository/"/>
  <setupTask
      xsi:type="setup:VariableTask"
      name="mwe2.update.site"
      value="http://download.eclipse.org/modeling/tmf/xtext/updates/composite/maintenance/nightly/"/>
  <setupTask
      xsi:type="setup:VariableTask"
      name="emf.update.site"
      value="http://download.eclipse.org/modeling/emf/emf/updates/2.12/"/>
  <setupTask
      xsi:type="setup:VariableTask"
      name="antlr-gen.update.site"
      value="http://download.itemis.com/updates/releases/2.1.1/"/>
  <setupTask
      xsi:type="setup:VariableTask"
      type="URI"
      name="draw2d.p2.repository"
      value="http://download.eclipse.org/tools/gef/updates/releases"/>
  <setupTask
      xsi:type="setup:VariableTask"
      name="eclipse.target.platform"
      defaultValue="Neon"
      storageURI="scope://Workspace"/>
  <setupTask
      xsi:type="setup:VariableTask"
      id=""
      name="orbit.clone.location"
      label="Git repositories location">
    <choice
        value="${workspace.location}"
        label="Located within the workspace folder"/>
    <choice
        value="${@id.location}"
        label="Located in the specified absolute folder location"/>
    <description>Several git repositories location will be cloned inside this directory</description>
  </setupTask>
  <setupTask
      xsi:type="setup.p2:P2Task"
      label="Team Tools">
    <requirement
        name="org.eclipse.emf.sdk.feature.group"/>
    <requirement
        name="org.eclipse.xtext.sdk.feature.group"/>
    <requirement
        name="org.eclipse.xtend.sdk.feature.group"/>
    <requirement
        name="org.eclipse.emf.mwe2.language.sdk.feature.group"/>
    <requirement
        name="org.eclipse.epp.mpc.feature.group"/>
    <requirement
        name="org.eclipse.pde.api.tools.ee.feature.feature.group"/>
    <requirement
        name="org.eclipse.pde.feature.group"/>
    <repository
        url="${xtext.nightly.composite}"/>
    <repository
        url="${mwe2.update.site}"/>
  </setupTask>
  <setupTask
      xsi:type="git:GitCloneTask"
      id="git.clone"
      filter=""
      location="${orbit.clone.location}/${git.clone.remoteURI|gitRepository}"
      remoteURI="https://github.com/eclipse-orbit/xtext-web.git">
    <description>${scope.project.label}</description>
  </setupTask>
  <setupTask
      xsi:type="setup.targlets:TargletTask">
    <targlet
        name="${scope.project.label}"
        activeRepositoryList="${eclipse.target.platform}"
        includeAllPlatforms="true">
      <annotation
          source="http:/www.eclipse.org/oomph/targlets/TargetDefinitionGenerator">
        <detail
            key="location">
          <value>${git.clone.location/releng/org.eclipse.xtext.web.target/org.eclipse.xtext.web.target.target}</value>
        </detail>
        <detail
            key="includeAllPlatforms">
          <value>true</value>
        </detail>
        <detail
            key="includeSource">
          <value>true</value>
        </detail>
        <detail
            key="generateVersions">
          <value>false</value>
        </detail>
        <detail
            key="generateImplicitUnits">
          <value>true</value>
        </detail>
        <detail
            key="includeMode">
          <value>slicer</value>
        </detail>
      </annotation>
      <annotation
          source="http:/www.eclipse.org/oomph/targlets/PomArtifactUpdater">
        <detail
            key="skipArtifactIDs">
          <value>false</value>
        </detail>
        <detail
            key="skipVersions">
          <value>false</value>
        </detail>
      </annotation>
      <requirement
          name="org.eclipse.equinox.sdk.feature.group"/>
      <requirement
          name="org.eclipse.equinox.http.jetty"/>
      <requirement
          name="org.eclipse.jetty.xml"/>
      <requirement
          name="org.eclipse.jetty.continuation"/>
      <requirement
          name="org.eclipse.jetty.webapp"/>
      <requirement
          name="org.eclipse.jetty.http"/>
      <requirement
          name="org.eclipse.jetty.io"/>
      <requirement
          name="org.eclipse.jetty.security"/>
      <requirement
          name="org.eclipse.jetty.server"/>
      <requirement
          name="org.eclipse.jetty.servlet"/>
      <requirement
          name="org.eclipse.jetty.util"/>
      <requirement
          name="org.eclipse.jetty.annotations"/>
      <requirement
          name="org.eclipse.jetty.annotations.source"/>
      <requirement
          name="javax.annotation"/>
      <requirement
          name="javax.persistence"/>
      <requirement
          name="javax.mail"/>
      <requirement
          name="org.eclipse.jetty.osgi.boot"/>
      <requirement
          name="org.eclipse.jetty.osgi-servlet-api"/>
      <requirement
          name="org.eclipse.jetty.osgi.httpservice"/>
      <requirement
          name="ch.qos.logback.classic"/>
      <requirement
          name="ch.qos.logback.core"/>
      <requirement
          name="ch.qos.logback.slf4j"/>
      <requirement
          name="org.eclipse.xtext.xbase"/>
      <requirement
          name="org.eclipse.xtext"/>
      <requirement
          name="org.eclipse.xtext.ecore"/>
      <requirement
          name="org.eclipse.xtext.util"/>
      <requirement
          name="org.eclipse.xtext.xbase.lib"/>
      <requirement
          name="org.eclipse.xtext.ide"/>
      <requirement
          name="org.eclipse.xtend.lib.macro"/>
      <requirement
          name="org.eclipse.xtend.lib"/>
      <requirement
          name="org.eclipse.xtext.xbase.ide"/>
      <requirement
          name="org.eclipse.xtext.xbase.ide"/>
      <requirement
          name="org.eclipse.xtext.xtext.generator"/>
      <requirement
          name="org.eclipse.emf.mwe2.launch"/>
      <requirement
          name="com.google.inject.assistedinject"/>
      <requirement
          name="com.google.inject.multibindings"/>
      <requirement
          name="com.google.inject.multibindings.source"/>
      <requirement
          name="com.google.inject"/>
      <requirement
          name="org.apache.log4j"/>
      <requirement
          name="org.slf4j.log4j.source"/>
      <requirement
          name="com.ibm.icu"/>
      <requirement
          name="org.eclipse.emf"/>
      <requirement
          name="org.eclipse.emf.codegen"/>
      <requirement
          name="org.eclipse.emf.codegen.ecore"/>
      <requirement
          name="com.google.gson"/>
      <requirement
          name="com.google.gson.source"/>
      <sourceLocator
          rootFolder="${git.clone.location}"
          locateNestedProjects="true"/>
      <repositoryList
          name="Neon">
        <repository
            url="http://download.eclipse.org/releases/neon"/>
        <repository
            url="http://download.eclipse.org/cbi/updates/license"/>
        <repository
            url="http://download.eclipse.org/jetty/updates/jetty-bundles-9.x/9.3.10.v20160621"/>
        <repository
            url="http://download.eclipse.org/tools/orbit/downloads/drops/R20160520211859/repository/"/>
        <repository
            url="http://agentlab.ru/p2/xtext/2.11.0/"/>
      </repositoryList>
      <repositoryList
          name="Mars">
        <repository
            url="http://download.eclipse.org/releases/mars"/>
        <repository
            url="http://download.eclipse.org/cbi/updates/license"/>
        <repository
            url="http://download.eclipse.org/jetty/updates/jetty-bundles-9.x/9.2.13.v20150730"/>
      </repositoryList>
      <repositoryList
          name="Luna">
        <repository
            url="http://download.eclipse.org/releases/luna"/>
        <repository
            url="http://download.eclipse.org/cbi/updates/license"/>
      </repositoryList>
      <repositoryList
          name="Kepler">
        <repository
            url="http://download.eclipse.org/releases/kepler"/>
        <repository
            url="http://download.eclipse.org/cbi/updates/license"/>
      </repositoryList>
      <repositoryList
          name="Juno">
        <repository
            url="http://download.eclipse.org/releases/juno"/>
        <repository
            url="http://download.eclipse.org/cbi/updates/license"/>
      </repositoryList>
    </targlet>
  </setupTask>
  <setupTask
      xsi:type="setup.workingsets:WorkingSetTask">
    <workingSet
        name="${scope.project.name}">
      <predicate
          xsi:type="predicates:LocationPredicate"
          pattern=".*/xtext-web/.*"/>
    </workingSet>
    <description>The dynamic working sets for ${scope.project.label}</description>
  </setupTask>
  <stream name="master"
      label="master"/>
  <logicalProjectContainer
      xsi:type="setup:ProjectCatalog"
      href="index:/org.eclipse.setup#//@projectCatalogs[name='org.eclipse']"/>
  <description>OSGi-based services for Xtext Web.</description>
</setup:Project>
